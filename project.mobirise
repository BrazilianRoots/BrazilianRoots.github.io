          "_name": "features1",
          "_customHTML": "<section class=\"features1 mbr-section\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"1\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"2\">\n\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n        <select title=\"Cards\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n        </select>\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background2.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#efefef\" selected>\n        </fieldset>\n    <!-- End block parameters -->\n    </mbr-parameters>\n    \n    <div class=\"container\">\n        <div class=\"mbr-row mbr-jc-c\">\n            <div class=\"card mbr-col-sm-12 mbr-col-md-6 mbr-px-3\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/queenlike-hair-products-4-bundles-human-hair-bundles-8-28-non-remy-weft-brazilian-hair-weave-800x800.jpg\" layout=\"responsive\" width=\"528\" height=\"528\" alt=\"body wave brazilian hair\" class=\"mobirise-loader\">\n                            <div placeholder class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/queenlike-hair-products-4-bundles-human-hair-bundles-8-28-non-remy-weft-brazilian-hair-weave-800x800.jpg\" alt=\"body wave brazilian hair\" title>\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box mbr-pt-4\">\n                        <h4 class=\"card-title mbr-fonts-style align-left mbr-light\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Product Information</h4>\n                        <p class=\"mbr-text mbr-fonts-style align-left mbr-pt-1\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">\n                           </p><p><b>Material Grade:</b> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;             Non-Remy\n<br><b>Bleachability:</b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;Darker Colours\n<br><b>Unit Weight:</b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; 100g (+/-5g) per piece\n<br><b>Length:</b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; 8 to 30 inches\n<br><b>Delivery:</b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; FREE. Shipping through DHL/Fedex/UPS*&nbsp;<br><b>Longest Hair Proportion:</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&gt;=5%\n<br><b>Texture:</b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;Body wave&nbsp;<br><b>Human Hair Type:</b>&nbsp; &nbsp; &nbsp; Brazilian\n<br><b>Hair Weft:</b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Machine Double Weft\n<br><b>Hair Colour:</b> &nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;Natural, can be dyed or bleached</p><br><p>Please consult our <a href=\"Shipping.html\"><b>Shipping</b></a> and<b> <a href=\"Refunds.html\"><b>Refunds &amp; Returns</b></a> </b>pages for estimated delivery times and more information pertaining to our refunds and returns policy.</p><br><p><i>*All items are only shipped to South Africa.</i><br></p><p></p><p></p>\n                        <p></p>\n                    </div>\n                </div>\n            </div>\n            \n            <div class=\"card mbr-col-sm-12 mbr-col-md-6 mbr-px-3 last-child\" mbr-if=\"cardsAmount > 1\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/bb-4-57-4-bundles-brazilian-body-wave-virgin-hair-afro-hair-products-natural-weave-1-800x800.jpg\" layout=\"responsive\" height=\"528\" width=\"528\" alt=\"body wave brazilian hair\" class=\"mobirise-loader\">\n                            <div placeholder class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/bb-4-57-4-bundles-brazilian-body-wave-virgin-hair-afro-hair-products-natural-weave-1-800x800.jpg\" alt=\"body wave brazilian hair\" title>\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box mbr-pt-4\">\n                        <h4 class=\"card-title mbr-fonts-style align-left mbr-light\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Place Your Order</h4>\n                        <p class=\"mbr-text mbr-fonts-style align-left mbr-pt-1\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">\n                            </p><section class=\"container-fluid\">\n\t<div class=\"row\">\n\t\t<div class=\"col-xs-12 col-md-6 col-md-offset-3\">\n\t\t\t<form action=\"mail.php\" method=\"POST\">\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<input type=\"text\" class=\"form-control\" placeholder=\"Full Name\" name=\"name\" required>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<input type=\"email\" class=\"form-control\" placeholder=\"Email Address\" name=\"email\" required>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<input type=\"tel\" class=\"form-control\" placeholder=\"Phone Number\" name=\"phone\" required>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<input type=\"text\" class=\"form-control\" placeholder=\"Address Line 1\" name=\"add1\" required>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<input type=\"text\" class=\"form-control\" placeholder=\"Address Line 2\" name=\"add2\" required>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<input type=\"text\" class=\"form-control\" placeholder=\"City\" name=\"city\" required>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<label for=\"provinceSelect\">Select Province</label>\n\t\t\t\t\t<select class=\"form-control\" id=\"provinceSelect\" name=\"province\">\n\t\t\t\t\t\t<option>Western Cape</option>\n\t\t\t\t\t\t<option>Eastern Cape</option>\n\t\t\t\t\t\t<option>Northern Cape</option>\n\t\t\t\t\t\t<option>Gauteng</option>\n\t\t\t\t\t\t<option>Limpopo</option>\n\t\t\t\t\t\t<option>Mpumalanga</option>\n\t\t\t\t\t\t<option>KwaZulu-Natal</option>\n\t\t\t\t\t\t<option>North West</option>\n\t\t\t\t\t\t<option>Free State</option>\n\t\t\t\t\t</select>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<input type=\"text\" class=\"form-control\" placeholder=\"Zip code\" name=\"zip\" required>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n    \t\t\t\t\t<label for=\"lengthSelect\">Select Length</label>\n    \t\t\t\t\t<select class=\"form-control\" id=\"lengthSelect\" onchange=\"calcTotal()\" name=\"length\" required>\n      \t\t\t\t\t\t<option value=\"1\">8 inch</option>\n      \t\t\t\t\t\t<option value=\"2\">10 inch</option>\n      \t\t\t\t\t\t<option value=\"3\">12 inch</option>\n      \t\t\t\t\t\t<option value=\"4\">14 inch</option>\n      \t\t\t\t\t\t<option value=\"5\">16 inch</option>\n\t\t\t\t\t\t<option value=\"6\">18 inch</option>\n\t\t\t\t\t\t<option value=\"7\">20 inch</option>\n\t\t\t\t\t\t<option value=\"8\">22 inch</option>\n\t\t\t\t\t\t<option value=\"9\">24 inch</option>\n\t\t\t\t\t\t<option value=\"10\">26 inch</option>\n\t\t\t\t\t\t<option value=\"11\">28 inch</option>\n\t\t\t\t\t\t<option value=\"12\">30 inch</option>\n    \t\t\t\t\t</select>\n  \t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<label for=\"bundleSelect\">Select Bundles</label>\n\t\t\t\t\t<select class=\"form-control\" id=\"bundleSelect\" onchange=\"calcTotal()\" name=\"bundle\" required>\n\t\t\t\t\t\t<option value=\"1\">1 bundle</option>\n\t\t\t\t\t\t<option value=\"2\">3 bundles</option>\n\t\t\t\t\t</select>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<label for=\"quantity\">Qty</label>\n\t\t\t\t\t<select class=\"form-control\" id=\"quantity\" onchange=\"calcTotal()\" name=\"quantity\" required>\n\t\t\t\t\t\t<option value=\"1\">1</option>\n\t\t\t\t\t\t<option value=\"2\">2</option>\n\t\t\t\t\t\t<option value=\"3\">3</option>\n\t\t\t\t\t\t<option value=\"4\">4</option>\n\t\t\t\t\t\t<option value=\"5\">5</option>\n\t\t\t\t\t\t<option value=\"6\">6</option>\n\t\t\t\t\t\t<option value=\"7\">7</option>\n\t\t\t\t\t\t<option value=\"8\">8</option>\n\t\t\t\t\t\t<option value=\"9\">9</option>\n\t\t\t\t\t\t<option value=\"10\">10</option>\n                  </select>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<label for=\"total\">Total R</label>\n\t\t\t\t\t<input readonly type=\"text\" id=\"total\" class=\"form-control\" value=\"216.87\">\n\t\t\t\t</div>\n\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t<input type=\"submit\" id=\"submit\" class=\"form-control btn btn-md btn-primary\" onclick=\"btns()\" value=\"Submit\">\n\t\t\t\t</div>\n\t\t\t</form>\n\t\t\t<div hidden id=\"paybtn\">\n\t\t\t\t<a id=\"pay\"><img src=\"https://www.payfast.co.za/images/buttons/dark-small-paynow.png\" width=\"165\" height=\"36\" alt=\"Pay\" title=\"Pay Now with PayFast\"></a>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n\t\n\t<script>\n\t\tfunction btns(){\n\t\t\ty = document.getElementById(\"paybtn\");\n\t\t\ty.style.display = 'block';\n\t\t};\t\t\n\t\t\n\t\tfunction calcTotal(){\n\t\t\ta = document.getElementById(\"lengthSelect\").value;\n\t\t\tb = document.getElementById(\"bundleSelect\").value;\n\t\t\tqty = document.getElementById(\"quantity\").value;\n\t\t\tprice = 0.0;\n\t\t\tif (b == 1){\n\t\t\t\tswitch (a){\n\t\t\t\t\tcase \"1\":\n\t\t\t\t\t\tprice = 216.87;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"2\":\n\t\t\t\t\t\tprice = 244.38;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"3\":\n\t\t\t\t\t\tprice = 283.09;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"4\":\n\t\t\t\t\t\tprice = 318.67;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"5\":\n\t\t\t\t\t\tprice = 372.65;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"6\":\n\t\t\t\t\t\tprice = 421.65;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"7\":\n\t\t\t\t\t\tprice = 473.50;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"8\":\n\t\t\t\t\t\tprice = 550.72;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"9\":\n\t\t\t\t\t\tprice = 631.23;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"10\":\n\t\t\t\t\t\tprice = 713.86;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"11\":\n\t\t\t\t\t\tprice = 825.20;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tdefault:\n\t\t\t\t\t\tprice = 954.55;\n\t\t\t\t\t\tbreak;\n\t\t\t\t};\n\t\t\t}\n\t\t\telse{\n\t\t\t\tswitch (a){\n\t\t\t\t\tcase \"1\":\n\t\t\t\t\t\tprice = 630.50;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"2\":\n\t\t\t\t\t\tprice = 685.99;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"3\":\n\t\t\t\t\t\tprice = 805.50;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"4\":\n\t\t\t\t\t\tprice = 910.25;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"5\":\n\t\t\t\t\t\tprice = 1003.55;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"6\":\n\t\t\t\t\t\tprice = 1150.90;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"7\":\n\t\t\t\t\t\tprice = 1333.10;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"8\":\n\t\t\t\t\t\tprice = 1590.20;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"9\":\n\t\t\t\t\t\tprice = 1860.95;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"10\":\n\t\t\t\t\t\tprice = 2100.58;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tcase \"11\":\n\t\t\t\t\t\tprice = 2425.60;\n\t\t\t\t\t\tbreak;\n\t\t\t\t\tdefault:\n\t\t\t\t\t\tprice = 2813.65;\n\t\t\t\t\t\tbreak;\n\t\t\t\t};\n\t\t\t}\n\t\t\ttotalP = Number((price*qty).toFixed(2));\n\t\t\tdocument.getElementById(\"total\").setAttribute(\"value\",totalP);\n\t\t\tstr_num = document.getElementById(\"total\").value;\n\t\t\tstr_link = \"https://www.payfast.co.za/eng/process?cmd=_paynow&receiver=15263013&item_name=Body+Wave+Bundle&amount=\"+str_num;\n\t\t\tdocument.getElementById(\"pay\").setAttribute(\"href\",str_link);\n\t\t\t};\n\n\t</script>\n</section>\n                        <p></p>\n                    </div>\n                </div>\n            </div>\n            \n        </div>\n    </div>\n</section>",
